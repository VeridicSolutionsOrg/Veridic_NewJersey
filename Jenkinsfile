    agent any
    stages{
        stage('checkout'){
    steps{
    sh 'git checkout Verdic/Sri3009'
    }
    }
    }
        stage('Build') {
            steps {
                sh 'echo "Task accomplished"'
                }
              }
         stage('Deploy'){
            steps{
              sh 'echo "deploy"'
             }
            }
       }
